<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="owl.C2Componente" owl="1">
        <div>
            <table class="table">
                <thead>
                    <tr>
                        <th>Subasta ID</th>
                        <th>Valor Inicial</th>
                    </tr>
                </thead>
                <tbody>
                    <tr t-foreach="state.subastas" t-as="subasta">
                        <td><t t-esc="subasta.name"/></td>
                        <td><t t-esc="subasta.valorInicial"/></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </t>
</templates>
