<odoo>
    <template id="inmobiliaria_upo_empresa_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <div class="page">
                    <t t-esc="doc.name" />
                    <t t-esc="doc.nombre" />
                    <t t-esc="doc.telefono" />
                    <t t-esc="doc.email" />
                </div>
            </t>
        </t>
    </template>
</odoo>